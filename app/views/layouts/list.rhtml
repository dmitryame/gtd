<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
	"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html>
<head>
	<title>user: <%= controller.action_name %></title>
	<%= stylesheet_link_tag "default" %>
	<%= javascript_include_tag :defaults %>
</head>
<body>
	<table width="100%">
		<tr>
			<td>
				<font size="+3">G</font><font size="-3">et</font><font size="+3">T</font><font size="-3">hings</font><font size="+3">D</font><font size="-3">one</font>
			</td>
			<td>
				<div class="banner">
					<img src="/images/custom-mode.jpg"/>
				</div>
			</td>
		</tr>
	</table>
	<p style="color: green"><%= flash[:notice] %></p>

	<div id="main">
		<table>
			<tr>
				<% listTypes = List.find(:all) %>
				<% for listType in listTypes %> 
					<td>
						<div class="listTypeItem">
							(3)
							<%= listType.name %>
							&nbsp;&nbsp;&nbsp;
						</div>
					</td>	
					
				<% end %>
			</tr>
		</table>
		<%= yield :layout %>
	</div>
	<br>
	<div class="footer">
		Copyright 2007
	</div>
</body>
</html>
